{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-s gen-data dev-only",
    "build": "run-s type-check run-gen build-only",
    "preview": "vite preview",
    "dev-only": "vite",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "build-gen": "tsc --build tsconfig.gen.json",
    "run-gen": "node --env-file=.env scripts/dist",
    "gen-data": "run-s build-gen run-gen",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/ script.mjs"
  },
  "dependencies": {
    "@vueuse/core": "^11.0.3",
    "dexie": "^4.0.8",
    "pinia": "^2.2.2",
    "sass": "^1.78.0",
    "vue": "^3.5.4",
    "vue-router": "^4.4.4",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.28.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "typescript": "~5.5.0",
    "vite": "^5.4.4",
    "vue-tsc": "^2.1.6"
  }
}
